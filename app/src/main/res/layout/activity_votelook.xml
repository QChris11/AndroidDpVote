<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.kencin.exam2.Votelook">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    >
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/No1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="音乐："/>
    <ProgressBar
        android:id="@+id/musicBar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:max="100"
        android:progress="22"
        android:layout_weight="1"
        />
    <TextView
        android:id="@+id/musicText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="15dp"
        android:paddingRight="5dp"
        />
</LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/No2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="舞蹈："/>
        <ProgressBar
            android:id="@+id/danceBar"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:max="100"
            android:progress="9"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/danceText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:paddingRight="5dp"

            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/No3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="动画："/>
        <ProgressBar
            android:id="@+id/animateBar"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:max="100"
            android:progress="35"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/animateText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:paddingRight="5dp"

            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/No4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="英语："/>
        <ProgressBar
            android:id="@+id/englishBar"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:max="100"
            android:progress="55"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/englishText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:paddingRight="5dp"

            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/No5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="日语："/>
        <ProgressBar
            android:id="@+id/japaneseBar"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:max="100"
            android:progress="30"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/japaneseText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:paddingRight="5dp"

            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/No6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="德语："/>
        <ProgressBar
            android:id="@+id/germanBar"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:max="100"
            android:progress="22"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/germanText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:paddingRight="5dp"

            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/No7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="法语："/>
        <ProgressBar
            android:id="@+id/franceBar"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:max="100"
            android:progress="28"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/franceText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:paddingRight="5dp"

            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/No8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="机械工程："/>
        <ProgressBar
            android:id="@+id/macheBar"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:max="100"
            android:progress="11"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/macheText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:paddingRight="5dp"

            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/No9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="光学工程："/>
        <ProgressBar
            android:id="@+id/lightBar"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:max="100"
            android:progress="15"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/lightText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:paddingRight="5dp"

            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/No10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="材料工程："/>
        <ProgressBar
            android:id="@+id/materialBar"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:max="100"
            android:progress="12"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/materialText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:paddingRight="5dp"

            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="50dp">
        <Button
            android:id="@+id/StoL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="从小到大排序"/>
        <Button
            android:id="@+id/LtoS"
            android:layout_marginLeft="50dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="从大到小排序"/>
    </LinearLayout>













</LinearLayout>
</android.support.constraint.ConstraintLayout>
